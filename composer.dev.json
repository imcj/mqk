{
    "name": "mqk/mqk",
    "authors": [
        {
            "name": "CJ",
            "email": "weicongju@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "mqk/message": "*",
        "monolog/monolog": "^1.23",
        "symfony/console": "^3.3",
        "ad7six/dsn": "^0.4.0",
        "symfony/event-dispatcher": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/property-access": "^3.3",
        "sentry/sentry": "^1.7",
        "fatrellis/process": "0.0.7",
        "symfony/yaml": "^3.3",
        "mqk/predis": "dev-master#f51f0a0c",
        "react/http": "^0.8.3",
        "guzzlehttp/guzzle": "^6.3@dev",
        "slim/slim": "^3.0"
    },
    "autoload":{
        "psr-4": {
            "MQK\\": [
                "src/MQK/",
                "test/MQK/"
            ]
        },
        "psr-0": {
            "": "src/"
        }
    },
    "bin": [
        "bin/mqk"
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "dev-master"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../message",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "git",
            "url": "https://github.com/imcj/predis"
        }

    ],
    "minimum-stability": "dev"
}
